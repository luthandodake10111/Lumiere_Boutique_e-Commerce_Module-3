= 0) {\n        this.transitioning = false;\n        this.currentIndex = this.slides.length;\n      } else {\n        this.transitioning = true;\n        this.currentIndex--;\n      }\n    },\n    goToSlide(index) {\n      this.transitioning = true;\n      this.currentIndex = index;\n    },\n    goToProducts() {\n      this.$router.push('/ProductsPage');\n    },\n    startAutoSlide() {\n      this.interval = setInterval(this.nextSlide, 5000);\n    },\n    stopAutoSlide() {\n      clearInterval(this.interval);\n    },\n    handleTransitionEnd() {\n      if (this.currentIndex >= this.slides.length) {\n        this.transitioning = false;\n        this.currentIndex = 0;\n      }\n    }\n  },\n  mounted() {\n    this.startAutoSlide();\n  },\n  beforeUnmount() {\n    this.stopAutoSlide();\n  }\n};","map":{"version":3,"names":["data","slides","currentIndex","transitioning","interval","computed","displayedSlides","methods","nextSlide","length","prevSlide","goToSlide","index","goToProducts","$router","push","startAutoSlide","setInterval","stopAutoSlide","clearInterval","handleTransitionEnd","mounted","beforeUnmount"],"sources":["C:\\LCA-Code-Work\\04 M3 - E-commerce\\Cloned github e-commerce\\Lumiere_Boutique_e-Commerce_Module-3\\src\\components\\CarouselPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"carousel-container\">\r\n    <div class=\"hero-overlay\">\r\n      <h2>Embrace Elegance, Wear Lumiere.</h2>\r\n      <p>Elevate your style with our premium collection.</p>\r\n      <router-link to=\"/ProductPage\">\r\n        <button class=\"btn\">Shop Now</button>\r\n      </router-link>    </div>\r\n\r\n    <div\r\n      class=\"carousel\"\r\n      :style=\"{ transform: `translateX(-${currentIndex * 100}%)`, transition: transitioning ? 'transform 0.5s ease-in-out' : 'none' }\"\r\n      @transitionend=\"handleTransitionEnd\"\r\n    >\r\n      <div v-for=\"(slide, index) in displayedSlides\" :key=\"index\" class=\"carousel-slide\">\r\n        <img :src=\"slide\" alt=