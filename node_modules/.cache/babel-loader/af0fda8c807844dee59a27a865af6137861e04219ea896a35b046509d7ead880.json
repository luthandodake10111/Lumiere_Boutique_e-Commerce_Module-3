{"ast":null,"code":"import NavbarPage from \"@/components/NavbarPage.vue\";\nimport CarouselPage from \"@/components/CarouselPage.vue\";\nimport FooterPage from \"@/components/FooterPage.vue\";\nexport default {\n  components: {\n    NavbarPage,\n    CarouselPage,\n    FooterPage\n  },\n  data() {\n    return {\n      socialLinks: {\n        instagram: \"https://instagram.com\",\n        twitter: \"https://twitter.com\",\n        tiktok: \"https://www.tiktok.com\"\n      },\n      features: [{\n        title: \"Premium Quality\",\n        description: \"Our scarves are crafted with the finest silk and cotton blends.\",\n        image: \"https://luthandodake10111.github.io/Lumiere_Boutique_e-Commerce_Module-3/Vero%20Social%20images%20and%20icons/Images/cotton%20silk%20scarfs/Brown.jpg\",\n        alt: \"Premium Quality\",\n        link: \"#premium-quality\",\n        size: \"180px\"\n      }, {\n        title: \"Elegant Designs\",\n        description: \"Timeless patterns that complement any outfit.\",\n        image: \"https://luthandodake10111.github.io/Lumiere_Boutique_e-Commerce_Module-3/Vero%20Social%20images%20and%20icons/Images/galary/elegant%20designs%20of%20silk%20scarves.jpg\",\n        alt: \"Elegant Designs\",\n        link: \"#elegant-designs\",\n        size: \"180px\"\n      }, {\n        title: \"Luxury Silk Scarves\",\n        description: \"Explore our finest collection of silk and cotton blend scarves, crafted for elegance.\",\n        image: \"https://luthandodake10111.github.io/Lumiere_Boutique_e-Commerce_Module-3/Vero%20Social%20images%20and%20icons/Images/carousel%20slides/silk%20cotton%20carousel%202.jpg\",\n        alt: \"Elegant Scarf\",\n        link: \"#premium-quality\",\n        size: \"190px\"\n      }, {\n        title: \"Worldwide Shipping\",\n        description: \"We deliver luxury right to your doorstep with efficiency.\",\n        image: \"https://luthandodake10111.github.io/Lumiere_Boutique_e-Commerce_Module-3/Vero%20Social%20images%20and%20icons/Images/galary/worldwide-shipping-icon-5.jpg\",\n        alt: \"Worldwide Shipping\",\n        link: \"#worldwide-shipping\",\n        size: \"190px\"\n      }]\n    };\n  }\n};","map":{"version":3,"names":["NavbarPage","CarouselPage","FooterPage","components","data","socialLinks","instagram","twitter","tiktok","features","title","description","image","alt","link","size"],"sources":["C:\\LCA-Code-Work\\04 M3 - E-commerce\\Cloned github e-commerce\\Lumiere_Boutique_e-Commerce_Module-3\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  \r\n    <NavbarPage />\r\n    <CarouselPage />\r\n    <router-view></router-view>\r\n\r\n    <!-- Features Section -->\r\n    <section class=\"features\">\r\n  <div class=\"grid-container\">\r\n    <div v-for=\"(feature, index) in features\" :key=\"index\" class=\"card\">\r\n      <img :src=\"feature.image\" \r\n        class=\"card-img-top\" \r\n        :alt=\"feature.alt\"\r\n        :style=\"{ width: feature.size, height: feature.size }\">\r\n\r\n      <div class=\"card-body\">\r\n        <h5 class=\"card-title\">{{ feature.title }}</h5>\r\n        <p class=\"card-text\">{{ feature.description }}</p>\r\n        <router-link to=\"/AboutPage\" class=\"btn btn-primary\">Read More</router-link>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</section>\r\n\r\n  <FooterPage />\r\n</template>\r\n\r\n<script>\r\n\r\nimport NavbarPage from \"@/components/NavbarPage.vue\";\r\nimport CarouselPage from \"@/components/CarouselPage.vue\";\r\nimport FooterPage from \"@/components/FooterPage.vue\";\r\n\r\n\r\nexport default {\r\n  components: {\r\n    NavbarPage,\r\n    CarouselPage,\r\n    FooterPage\r\n  },\r\n  data() {\r\n  return {\r\n    socialLinks: {\r\n      instagram: \"https://instagram.com\",\r\n      twitter: \"https://twitter.com\",\r\n      tiktok: \"https://www.tiktok.com\"\r\n    },\r\n    \r\n    features: [\r\n      {\r\n        title: \"Premium Quality\",\r\n        description: \"Our scarves are crafted with the finest silk and cotton blends.\",\r\n        image: \"https://luthandodake10111.github.io/Lumiere_Boutique_e-Commerce_Module-3/Vero%20Social%20images%20and%20icons/Images/cotton%20silk%20scarfs/Brown.jpg\",\r\n        alt: \"Premium Quality\",\r\n        link: \"#premium-quality\",\r\n        size: \"180px\" \r\n      },\r\n      {\r\n        title: \"Elegant Designs\",\r\n        description: \"Timeless patterns that complement any outfit.\",\r\n        image: \"https://luthandodake10111.github.io/Lumiere_Boutique_e-Commerce_Module-3/Vero%20Social%20images%20and%20icons/Images/galary/elegant%20designs%20of%20silk%20scarves.jpg\",\r\n        alt: \"Elegant Designs\",\r\n        link: \"#elegant-designs\",\r\n        size: \"180px\"\r\n      },\r\n      {\r\n        title: \"Luxury Silk Scarves\",\r\n        description: \"Explore our finest collection of silk and cotton blend scarves, crafted for elegance.\",\r\n        image: \"https://luthandodake10111.github.io/Lumiere_Boutique_e-Commerce_Module-3/Vero%20Social%20images%20and%20icons/Images/carousel%20slides/silk%20cotton%20carousel%202.jpg\",\r\n        alt: \"Elegant Scarf\",\r\n        link: \"#premium-quality\",\r\n        size: \"190px\"\r\n      },\r\n      {\r\n        title: \"Worldwide Shipping\",\r\n        description: \"We deliver luxury right to your doorstep with efficiency.\",\r\n         image: \"https://luthandodake10111.github.io/Lumiere_Boutique_e-Commerce_Module-3/Vero%20Social%20images%20and%20icons/Images/galary/worldwide-shipping-icon-5.jpg\",\r\n        alt: \"Worldwide Shipping\",\r\n        link: \"#worldwide-shipping\",\r\n        size: \"190px\"\r\n      }\r\n    ]\r\n  };\r\n}\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\r\n/* Features Section */\r\n.grid-container {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\r\n  gap: 1.5rem;\r\n  max-width: 1200px;\r\n  margin: auto;\r\n  padding: 2rem;\r\n}\r\n\r\n.card {\r\n  background: whitesmoke !important;\r\n  padding: 1.5rem;\r\n  margin: 2rem auto;\r\n  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);\r\n  border-radius: 8px;\r\n  font-size: medium;\r\n}\r\n\r\n.card img {\r\n  width: 100%;\r\n  height: auto;\r\n  border-radius: 8px;\r\n  object-fit: cover;\r\n}\r\n\r\n.card h5 {\r\n  font-size: 20px !important;\r\n}\r\n\r\n.btn {\r\n  padding: 0.75rem 1.5rem;\r\n  background-color: black;\r\n  border: none;\r\n  border-radius: 115px;\r\n  cursor: pointer;\r\n  font-size: 1rem;\r\n  color: white;\r\n}\r\n\r\n.btn:hover {\r\n  background-color: #D4AF37;\r\n}\r\n\r\n/* Footer Styles */\r\n.footer {\r\n  background-color: whitesmoke;\r\n  /* color: #333333; */\r\n  padding: 2rem;\r\n}\r\n\r\n.footer-content {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  gap: 16px;\r\n}\r\n\r\n.footer-section {\r\n  flex: 1;\r\n}\r\n\r\n.footer h3 {\r\n  margin-bottom: 12px;\r\n  color: #D4AF37;\r\n}\r\n\r\n.footer p, .footer a {\r\n  font-size: 14px;\r\n  color: #333333;;\r\n}\r\n\r\n.footer a {\r\n  text-decoration: underline;\r\n}\r\n\r\n.footer a:hover {\r\n  color: #D4AF37;\r\n}\r\n/* Styling for Contact Info */\r\n.contact-info {\r\n  color: whitesmoke; \r\n\r\n}\r\n\r\n.social-media {\r\n  display: flex;\r\n  gap: 12px;\r\n}\r\n\r\n.social-icon {\r\n  font-size: 16px;\r\n  color: whitesmoke;\r\n}\r\n\r\n.social-icon:hover {\r\n  color: #D4AF37;\r\n}\r\n\r\n.footer-bottom {\r\n  text-align: center;\r\n  margin-top: 16px;\r\n  font-size: 14px;\r\n  color: #bbb;\r\n}\r\n\r\n.card-img-top {\r\n  width: 100px; \r\n  height: 100px;\r\n  object-fit: cover;\r\n  border-radius: 10px; \r\n}\r\n\r\n\r\n</style>\r\n"],"mappings":"AA6BA,OAAOA,UAAS,MAAO,6BAA6B;AACpD,OAAOC,YAAW,MAAO,+BAA+B;AACxD,OAAOC,UAAS,MAAO,6BAA6B;AAGpD,eAAe;EACbC,UAAU,EAAE;IACVH,UAAU;IACVC,YAAY;IACZC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACP,OAAO;MACLC,WAAW,EAAE;QACXC,SAAS,EAAE,uBAAuB;QAClCC,OAAO,EAAE,qBAAqB;QAC9BC,MAAM,EAAE;MACV,CAAC;MAEDC,QAAQ,EAAE,CACR;QACEC,KAAK,EAAE,iBAAiB;QACxBC,WAAW,EAAE,iEAA