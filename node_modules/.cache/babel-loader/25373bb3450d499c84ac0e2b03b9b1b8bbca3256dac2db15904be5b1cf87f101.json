 deliver luxury right to your doorstep with efficiency.\",\r\n         image: \"https://luthandodake10111.github.io/Lumiere_Boutique_e-Commerce_Module-3/Vero%20Social%20images%20and%20icons/Images/galary/worldwide-shipping-icon-5.jpg\",\r\n        alt: \"Worldwide Shipping\",\r\n        link: \"#worldwide-shipping\",\r\n        size: \"190px\"\r\n      }\r\n    ]\r\n  };\r\n}\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\r\n/* Features Section */\r\n.grid-container {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\r\n  gap: 1.5rem;\r\n  max-width: 1200px;\r\n  margin: auto;\r\n  padding: 2rem;\r\n}\r\n\r\n.card {\r\n  background: whitesmoke !important;\r\n  padding: 1.5rem;\r\n  margin: 2rem auto;\r\n  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);\r\n  border-radius: 8px;\r\n  font-size: medium;\r\n}\r\n\r\n.card img {\r\n  width: 100%;\r\n  height: auto;\r\n  border-radius: 8px;\r\n  object-fit: cover;\r\n}\r\n\r\n.card h5 {\r\n  font-size: 20px !important;\r\n}\r\n\r\n.btn {\r\n  padding: 0.75rem 1.5rem;\r\n  background-color: black;\r\n  border: none;\r\n  border-radius: 115px;\r\n  cursor: pointer;\r\n  font-size: 1rem;\r\n  color: white;\r\n}\r\n\r\n.btn:hover {\r\n  background-color: #D4AF37;\r\n}\r\n\r\n/* Footer Styles */\r\n.footer {\r\n  background-color: whitesmoke;\r\n  /* color: #333333; */\r\n  padding: 2rem;\r\n}\r\n\r\n.footer-content {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  gap: 16px;\r\n}\r\n\r\n.footer-section {\r\n  flex: 1;\r\n}\r\n\r\n.footer h3 {\r\n  margin-bottom: 12px;\r\n  color: #D4AF37;\r\n}\r\n\r\n.footer p, .footer a {\r\n  font-size: 14px;\r\n  color: #333333;;\r\n}\r\n\r\n.footer a {\r\n  text-decoration: underline;\r\n}\r\n\r\n.footer a:hover {\r\n  color: #D4AF37;\r\n}\r\n/* Styling for Contact Info */\r\n.contact-info {\r\n  color: whitesmoke; \r\n\r\n}\r\n\r\n.social-media {\r\n  display: flex;\r\n  gap: 12px;\r\n}\r\n\r\n.social-icon {\r\n  font-size: 16px;\r\n  color: whitesmoke;\r\n}\r\n\r\n.social-icon:hover {\r\n  color: #D4AF37;\r\n}\r\n\r\n.footer-bottom {\r\n  text-align: center;\r\n  margin-top: 16px;\r\n  font-size: 14px;\r\n  color: #bbb;\r\n}\r\n\r\n.card-img-top {\r\n  width: 100px; \r\n  height: 100px;\r\n  object-fit: cover;\r\n  border-radius: 10px; \r\n}\r\n\r\n\r\n</style>\r\n"],"mappings":";;EAOaA,KAAK,EAAC;AAAU;;EACtBA,KAAK,EAAC;AAAgB;mBAR7B;;EAeWA,KAAK,EAAC;AAAW;;EAChBA,KAAK,EAAC;AAAY;;EACnBA,KAAK,EAAC;AAAW;;;;;;;uBAhB1BC,mBAAA,CAwBI,cAvBFC,YAAA,CAAcC,qBAAA,GACdD,YAAA,CAAgBE,uBAAA,GAChBF,YAAA,CAA2BG,sBAAA,GAE3BC,mBAAA,sBAAyB,EACzBC,mBAAA,CAeM,WAfNC,UAeM,GAdRD,mBAAA,CAaM,OAbNE,UAaM,I,kBAZJR,mBAAA,CAWMS,SAAA,QApBVC,WAAA,CASoCC,KAAA,CAAAC,QAAQ,EAT5C,CASiBC,OAAO,EAAEC,KAAK;yBAA3Bd,mBAAA,CAWM;MAXqCe,GAAG,EAAED,KAAK;MAAEf,KAAK,EAAC;QAC3DO,mBAAA,CAGyD;MAHnDU,GAAG,EAAEH,OAAO,CAACI,KAAK;MACtBlB,KAAK,EAAC,cAAc;MACnBmB,GAAG,EAAEL,OAAO,CAACK,GAAG;MAChBC,KAAK,EAbdC,eAAA;QAAAC,KAAA,EAayBR,OAAO,CAACS,IAAI;QAAAC,MAAA,EAAUV,OAAO,CAACS;MAAI;oCAb3DE,UAAA,GAeMlB,mBAAA,CAIM,OAJNmB,UAIM,GAHJnB,mBAAA,CAA+C,MAA/CoB,UAA+C,EAAAC,gBAAA,CAArBd,OAAO,CAACe,KAAK,kBACvCtB,mBAAA,CAAkD,KAAlDuB,UAAkD,EAAAF,gBAAA,CAA1Bd,OAAO,CAACiB,WAAW,kBAC3C7B,YAAA,CAA4E8B,sBAAA;MAA/DC,EAAE,EAAC,YAAY;MAACjC,KAAK,EAAC;;MAlB3CkC,OAAA,EAAAC,QAAA,CAkB6D,MAAS,KAAAC,MAAA,QAAAA,MAAA,OAlBtEC,gBAAA,CAkB6D,WAAS,E;MAlBtEC,CAAA;;sCAwBEpC,YAAA,CAAcqC,qBAAA,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}